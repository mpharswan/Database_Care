Database Assignment queries:


CREATE TABLE:


CREATE TABLE `beneficiary` (
	`beneficiary_ID` INT(11) NOT NULL AUTO_INCREMENT,
	`Project_ID` INT(11) NOT NULL,
	`PROGRAM_ID` INT(11) NOT NULL,
	`Beneficiary_Name` VARCHAR(255) NOT NULL COLLATE 'latin1_swedish_ci',
	`DOB` DATE NOT NULL,
	PRIMARY KEY (`beneficiary_ID`) USING BTREE,
	INDEX `Project_ID` (`Project_ID`) USING BTREE,
	INDEX `PROGRAM_ID` (`PROGRAM_ID`) USING BTREE,
	CONSTRAINT `beneficiary_ibfk_1` FOREIGN KEY (`Project_ID`) REFERENCES `projects` (`Project_ID`) ON UPDATE RESTRICT ON DELETE RESTRICT,
	CONSTRAINT `beneficiary_ibfk_2` FOREIGN KEY (`PROGRAM_ID`) REFERENCES `programs` (`PROGRAM_ID`) ON UPDATE RESTRICT ON DELETE RESTRICT
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB
AUTO_INCREMENT=11
;





CREATE TABLE `country` (
	`COUNTRY_ID` INT(11) NOT NULL AUTO_INCREMENT,
	`COUNTRY_NAME` VARCHAR(255) NOT NULL COLLATE 'latin1_swedish_ci',
	`PROGRAM_ID` INT(11) NOT NULL,
	PRIMARY KEY (`COUNTRY_ID`) USING BTREE,
	INDEX `PROGRAM_ID` (`PROGRAM_ID`) USING BTREE,
	CONSTRAINT `country_ibfk_1` FOREIGN KEY (`PROGRAM_ID`) REFERENCES `programs` (`PROGRAM_ID`) ON UPDATE RESTRICT ON DELETE RESTRICT
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB
AUTO_INCREMENT=11
;




CREATE TABLE `multimedia` (
	`MULTIMEDIA_ID` INT(11) NOT NULL AUTO_INCREMENT,
	`PROGRAM_ID` INT(11) NOT NULL,
	`PROGRAM_NAME` VARCHAR(255) NOT NULL COLLATE 'latin1_swedish_ci',
	`MEDIA` VARCHAR(255) NOT NULL COLLATE 'latin1_swedish_ci',
	PRIMARY KEY (`MULTIMEDIA_ID`) USING BTREE,
	INDEX `PROGRAM_ID` (`PROGRAM_ID`) USING BTREE,
	CONSTRAINT `multimedia_ibfk_1` FOREIGN KEY (`PROGRAM_ID`) REFERENCES `programs` (`PROGRAM_ID`) ON UPDATE RESTRICT ON DELETE RESTRICT
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB
AUTO_INCREMENT=11
;




CREATE TABLE `programs` (
	`PROGRAM_id` INT(11) NOT NULL,
	`PROGRAM_NAME` VARCHAR(255) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`SPONSOR_ID` INT(11) NULL DEFAULT NULL,
	PRIMARY KEY (`PROGRAM_id`) USING BTREE,
	INDEX `SPONSOR_ID` (`SPONSOR_ID`) USING BTREE,
	CONSTRAINT `programs_ibfk_1` FOREIGN KEY (`SPONSOR_ID`) REFERENCES `support` (`Sponsor_ID`) ON UPDATE RESTRICT ON DELETE RESTRICT
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB
;




CREATE TABLE `projects` (
	`PROJECT_ID` INT(11) NOT NULL,
	`PROGRAM_ID` INT(11) NULL DEFAULT NULL,
	`PROJECT_NAME` VARCHAR(255) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`PROJECT_DESCRIPTION` VARCHAR(255) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`country_ID` INT(11) NULL DEFAULT NULL,
	`Project_Reach` INT(11) NULL DEFAULT NULL,
	PRIMARY KEY (`PROJECT_ID`) USING BTREE,
	INDEX `PROGRAM_ID` (`PROGRAM_ID`) USING BTREE,
	INDEX `country_ID` (`country_ID`) USING BTREE,
	CONSTRAINT `projects_ibfk_1` FOREIGN KEY (`PROGRAM_ID`) REFERENCES `programs` (`PROGRAM_id`) ON UPDATE RESTRICT ON DELETE RESTRICT,
	CONSTRAINT `projects_ibfk_2` FOREIGN KEY (`country_ID`) REFERENCES `country` (`COUNTRY_ID`) ON UPDATE RESTRICT ON DELETE RESTRICT
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB




CREATE TABLE `research` (
	`Research_ID` INT(11) NOT NULL AUTO_INCREMENT,
	`Country_ID` INT(11) NOT NULL,
	`Research_Topic` VARCHAR(255) NOT NULL COLLATE 'latin1_swedish_ci',
	`File_Type` VARCHAR(255) NOT NULL COLLATE 'latin1_swedish_ci',
	PRIMARY KEY (`Research_ID`) USING BTREE,
	INDEX `Country_ID` (`Country_ID`) USING BTREE,
	CONSTRAINT `research_ibfk_1` FOREIGN KEY (`Country_ID`) REFERENCES `country` (`COUNTRY_ID`) ON UPDATE RESTRICT ON DELETE RESTRICT
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB
AUTO_INCREMENT=11
;




CREATE TABLE `support` (
	`Sponsor_ID` INT(11) NOT NULL AUTO_INCREMENT,
	`Type_Of_Sponsor` VARCHAR(255) NOT NULL COLLATE 'latin1_swedish_ci',
	PRIMARY KEY (`Sponsor_ID`) USING BTREE
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB
AUTO_INCREMENT=111
;



